{
  "name": "agvisor-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www.js",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max-http-header-size=64000 supervisor -i node_modules -w . -e graphqls,md,js bin/www.js",
    "clean": "rimraf .env public/.env public/styles/style.* public/scripts/bundle.js public/scripts/bundle.js.map public/service-bindings.js public/deployable-version.json coverage",
    "nonProd:package": "npm run clean && cross-env DEPLOYABLE_ENV=nonProd NODE_ENV=production webpack --mode production && npm pack && rimraf .env",
    "nonProd:create": "npm run nonProd:package && le-deploy create --projectId e15e62f0-aa7c-11ee-ba36-37c96eac91a6 --projectName $npm_package_name --team <teamId> --environment NonProd --contextRoot agvisor-ui --deployableVersion $npm_package_version --pipelineClientId <pipelineClientId> --notificationsUrl <teamsWebhookUrl>",
    "nonProd:deploy": "npm run nonProd:package && le-deploy deploy --projectId e15e62f0-aa7c-11ee-ba36-37c96eac91a6 --projectName $npm_package_name --environment NonProd --deployableVersion $npm_package_version",
    "nonProd:rollback": "le-deploy rollback --projectId e15e62f0-aa7c-11ee-ba36-37c96eac91a6 --environment NonProd --deployableVersion $npm_package_version",
    "nonProd:checkVersion": "le-deploy checkVersion --projectId e15e62f0-aa7c-11ee-ba36-37c96eac91a6 --environment NonProd",
    "nonProd:checkDeployableStatus": "le-deploy checkDeployableStatus --projectId e15e62f0-aa7c-11ee-ba36-37c96eac91a6 --environment NonProd",
    "lint": "cross-env eslint .",
    "lint:fix": "cross-env eslint --fix .",
    "test": "NODE_ENV=test mocha test",
    "test:log": "NODE_ENV=test mocha --timeout 50000 -R json-stream test > mocha.log"
  },
  "description": "",
  "deployable": {
    "cmdb": "agvisor-ui-aws",
    "projectId": "e15e62f0-aa7c-11ee-ba36-37c96eac91a6",
    "projectName": "agvisor-ui",
    "contextRoot": "agvisor-ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@monsantoit/config": "~3.0.3",
    "@monsantoit/phoenix-html-middleware": "~3.0.0",
    "@monsantoit/phoenix-service-bindings-middleware": "~4.0.0",
    "@monsantoit/ping-page": "~3.0.0",
    "compression": "~1.7.4",
    "dotenv": "~16.3.1",
    "express": "~4.18.2",
    "lodash": "~4.17.21",
    "moment": "~2.30.1"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@babel/core": "~7.23.7",
    "@babel/plugin-proposal-object-rest-spread": "~7.20.7",
    "@babel/preset-env": "~7.23.7",
    "@babel/preset-react": "~7.23.3",
    "@babel/register": "~7.23.7",
    "@monsantoit/phoenix-navbar": "~3.5.7",
    "@monsantoit/profile-client": "~5.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "~0.5.11",
    "babel-loader": "~9.1.3",
    "babel-plugin-autobind-class-methods": "~5.0.1",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "chai": "~5.0.0",
    "chai-as-promised": "~7.1.1",
    "core-js": "~3.35.0",
    "cross-env": "~7.0.3",
    "css-loader": "~6.8.1",
    "deep-freeze": "~0.0.1",
    "eslint": "~8.56.0",
    "eslint-config-airbnb": "~19.0.4",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-jsx-a11y": "~6.8.0",
    "eslint-plugin-react": "~7.33.2",
    "file-loader": "~6.2.0",
    "husky": "~8.0.3",
    "jsdom": "~23.0.1",
    "json-loader": "~0.5.7",
    "mini-css-extract-plugin": "~2.7.6",
    "mocha": "~10.2.0",
    "mocha-prepare": "~0.1.0",
    "prettier": "~3.1.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "~9.0.4",
    "react-refresh": "~0.14.0",
    "react-router-dom": "~6.21.1",
    "redux": "~5.0.1",
    "redux-thunk": "~2.4.2",
    "regenerator-runtime": "~0.14.1",
    "rimraf": "~5.0.5",
    "sass": "~1.39.2",
    "sass-loader": "~13.3.3",
    "sinon": "~17.0.1",
    "sinon-chai": "~3.7.0",
    "style-loader": "~3.3.3",
    "supervisor": "~0.12.0",
    "terser-webpack-plugin": "~5.3.10",
    "webpack": "~5.89.0",
    "webpack-cli": "~5.1.4",
    "webpack-dev-middleware": "~7.0.0",
    "webpack-hot-middleware": "~2.26.0"
  }
}
